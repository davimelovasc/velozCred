<header>
  <% if action_name == "search" %>
  <%= link_to 'Voltar', 'javascript:history.back()' %>
  <% end %>
  <%= link_to "Sair", destroy_agent_session_path, method: :delete, class: "float-md-right",style: "margin-right: 2%;", data: {confirm: "Você tem certeza que deseja encerrar sua sessão?"} %>
  <% if current_agent.role == "Adm" %>
  <%= link_to "Atualizar Banco de Dados", "", class: "float-md-right", 'data-toggle': "modal", 'data-target': "#exampleModal"%>
  <% end %>
  <%= link_to "Redefinir Senha", change_password_path(current_agent.id), class: "float-md-right" %>
</header>

<% if current_agent.role == "Adm" %>
  <%= render partial: "layouts/partials/update_modal" %>

<% end %>
